    "copyfiles": "^2.4.1",
    "electron": "^16.0.7",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "moment": "^2.29.1",
    "uuid": "^8.3.2"
